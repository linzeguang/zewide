{
  "name": "zewide",
  "version": "0.0.12",
  "description": "小前端的组件库 + 方法库",
  "keywords": [
    "utils",
    "component",
    "web3"
  ],
  "homepage": "https://github.com/linzeguang/zewide#readme",
  "bugs": {
    "url": "https://github.com/linzeguang/zewide/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linzeguang/zewide.git"
  },
  "license": "MIT",
  "author": "linzeguang",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf ./dist && rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist",
    "build:docs": "build-storybook -o docs",
    "docs": "start-storybook -p 6006",
    "format": "yarn lint && yarn prettier",
    "lint": "npx eslint src --cache --fix",
    "prettier": "npx prettier src --check --write",
    "start": "yarn docs",
    "watch": "rollup -c -w"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/onboarding": "^1.0.1",
    "@metamask/providers": "^9.1.0",
    "@walletconnect/ethereum-provider": "^1.8.0",
    "@web3-react/core": "^8.0.35-beta.0",
    "@web3-react/metamask": "^8.0.28-beta.0",
    "@web3-react/types": "^8.0.20-beta.0",
    "@web3-react/walletconnect": "^8.0.35-beta.0",
    "bignumber.js": "^9.1.0",
    "framer-motion": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-storage-hooks": "^4.0.1",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@rollup/plugin-typescript": "^8.3.4",
    "@rollup/plugin-url": "^7.0.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.15",
    "@umijs/fabric": "^3.0.0",
    "babel-loader": "^8.2.5",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "rollup": "^2.78.0",
    "typescript": "^4.7.4"
  }
}
